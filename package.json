{
  "name": "@dz-web/message",
  "version": "1.0.7",
  "description": "An awesome libary",
  "main": "index.js",
  "keywords": [
    "message",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "frontend"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run clean && webpack --config ./dev/webpack.config.js",
    "example": "webpack --config ./dev/webpack.example.js",
    "start": "node ./dev/serve.js",
    "prepublishOnly": "pnpm run build && pnpm run example",
    "pub": "npm publish --access public",
    "test": "node ./dev/jest/start.js",
    "test:coverage": "npm run test -- --coverage",
    "ghpages": "npm run example && git subtree push --prefix example-dist/ origin gh-pages",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git"
  },
  "author": "moonlit",
  "license": "MIT",
  "files": [
    "dist",
    "index.js",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.3",
    "raf": "^3.4.0",
    "rimraf": "^2.6.2",
    "sass": "^1.63.3",
    "sass-loader": "^10.1.1",
    "style-loader": "^0.20.3",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.2",
    "webpack-hot-client": "^4.2.0",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^0.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/dev/jest/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/style-mock.js"
    },
    "testMatch": [
      "<rootDir>/**/__tests__/**/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ]
  },
  "volta": {
    "node": "14.20.1"
  }
}
